<table>
  <tr>
    <th>Name</th><th>Email</th><th>Twitter</th>
  </tr>
  
<% session['users'].each_with_index do | value, index | %>
  <tr  onclick="location.href='/suckers/<%= index %>'" >
    
    <td> <%= value[:name] %> </td> 

    <td><%=  value[:email] %> </td> 

    <td><%=  value[:twitter] %> </td>
  </tr>
  <% end %>
</table>
